
<% @user = SetLifts.all %>
<% user = @user.where(:sign_up_id => session[:user_id]).last %>
<div id="center-col">


<h3 style="margin-left:50%;"><%= user.program %></h3>
<div class="bs-example">
    <div class="panel-group" id="accordion">
        <div class="panel panel-default">
            <div class="panel-heading">
                <h4 class="panel-title">
                    <a data-toggle="collapse" data-parent="#accordion" href="#collapseOne">Week One</a>
                </h4>
            </div>
            <div id="collapseOne" class="panel-collapse collapse in">
                <div class="panel-body">
                  <table class="table table-hover">
                    <thead>
                      <tr>
                        <th>Lift</th>
                        <th>First Set</th>
                        <th>Second Set</th>
                        <th>Third Set</th>
                      </tr>
                    </thead>
                    <tbody>
                      <tr>
                        <td>Squat</td>
                        <td><%= (0.9*user.Squat*0.75).round %>lbx5 </td>
                        <td><%= (0.9*user.Squat*0.80).round %>lbx5 </td>
                        <td><%= (0.9*user.Squat*0.85).round %>lbx5 </td>
                      </tr>
                      <tr>
                        <td>Bench</td>
                        <td><%= (0.9*user.Bench*0.75).round %>lbx5 </td>
                        <td><%= (0.9*user.Bench*0.80).round %>lbx5 </td>
                        <td><%= (0.9*user.Bench*0.85).round %>lbx5 </td>
                      </tr>
                      <tr>
                        <td>Dead</td>
                        <td><%= (0.9*user.Dead*0.75).round %>lbx5 </td>
                        <td><%= (0.9*user.Dead*0.80).round %>lbx5 </td>
                        <td><%= (0.9*user.Dead*0.85).round %>lbx5 </td>
                      </tr>
                    </tbody>
                  </table>
                </div>
            </div>
        </div>
